<% if @comment.errors.any? %>
  $(".error-window").html("<em>Body cannot be blank!</em>");

<% else %>

  $("#discussions").html("<%= j render partial: "/discussions/discussions_table", locals: { project: @project, discussions: @project.discussions.where(status: false)} %>");

  <% @discussion = Discussion.new %> 
  
  $("#discussion-form").html("<%= j render partial: "/discussions/form" %>");
<% end %>